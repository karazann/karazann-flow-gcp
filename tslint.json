{
    "extends": ["tslint:recommended", "tslint-config-prettier"],
    "rules": {
        "ordered-imports": [false],
        "no-console": [false],
        "curly": [true, "ignore-same-line"],
        "object-literal-sort-keys": [false],
        "no-string-literal": [false],
        "max-classes-per-file": [true, 4],
        "semicolon": [true, "never"]
    }
}